<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ page import="com.qfedu.lvyou.model.User"%>
<%@ page import="java.util.List"%>
<div>
	<form method="post" class="form-x" id="woUserImportForm" enctype="multipart/form-data">
		<div class="form-group">
			<div class="label">
				<label for="importFile">数据文件</label>
			</div>
			<div class="field">
				<a class="button input-file">+上传文件<input size="100" type="file"
					name="importFile" id="importFile" />
				</a>
			</div>
		</div>
		<div class="form-button">
			<button class="button bg-main icon-save" type="button"
				onclick="Wobb.submitUserImportForm();"> 上传</button>
			<button class="button bg-yellow icon-arrow-circle-right"
				type="button" onclick="WoKit.destroyMainFrame();"> 取消</button>
		</div>
	</form>
</div>